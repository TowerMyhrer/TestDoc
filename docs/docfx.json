{
    "build": {
      "content": [
        {
          "files": [
		  			"custvend/**.md",
						"custvend/**/toc.yml",
						"pdffiles/**.md",
						"pdffiles/**/toc.yml",
						"rolecenter/**.md",
						"rolecenter/**/toc.yml",
						"roles/**.md",
						"roles/**/toc.yml",
						"search/**.md",
						"search/**/toc.yml",
						"setup/**.md",
						"setup/**/toc.yml",
						"subunits/**.md",
						"subunits/**/toc.yml",
						"updates/**.md",
						"updates/**/toc.yml",
						"toc.yml",
						"*.md"
					]
        }
      ],
      "resource": [
        {
          "files": [
            "media/**"
					]
        }
      ],
      "output": "_site",
      "globalMetadataFiles": [],
      "fileMetadataFiles": [],
      "template": [
        "default",
        "modern"
      ],
      "postProcessors": [],
      "keepFileLink": false,
      "globalMetadata": {
        "_appLogoPath": "media/logo.svg",
        "_appFaviconPath": "media/favicon.ico",
				"pdfCoverPage": "cover.html",
				"pdfTocPage": true,
				"pdf": true
      },
      "disableGitFeatures": false
    }
  }