{
    "build": {
      "content": [
        {
          "files": [
		  			"Changes/**.md",
						"Changes/**/toc.yml",
						"DocApi/**.md",
						"DocApi/**/toc.yml",
						"FinanceImport/**.md",
						"FinanceImport/**/toc.yml",
						"toc.yml",
						"*.md"
					]
        }
      ],
      "resource": [
        {
          "files": [
            "media/**"
					]
        }
      ],
      "output": "_site",
      "globalMetadataFiles": [],
      "fileMetadataFiles": [],
      "template": [
        "default",
        "modern"
      ],
      "postProcessors": [],
      "keepFileLink": false,
      "globalMetadata": {
        "_appLogoPath": "media/logo.svg",
        "_appFaviconPath": "media/favicon.ico",
				"pdfCoverPage": "cover.html",
				"pdfTocPage": true,
				"pdf": true
      },
      "disableGitFeatures": false
    }
  }